import re

# 示例数据
data = "read:Success一些内容abcd"  # 或者 "read:Successabcd"（没有内容）

# 正则表达式：匹配 read:Success 和 abcd 之间的数据
pattern = r"read:Success(.*?)abcd"

# 使用 re.search 提取匹配的部分
match = re.search(pattern, data)

if match:
    extracted_data = match.group(1)  # 提取括号中的部分
    print(f"提取的数据: '{extracted_data}'")
else:
    print("没有找到匹配的数据")
