import re

def count_pattern_occurrences(s):
    # 正则表达式匹配 "Writing to glpa [数字] failed."，忽略方括号中的数字
    pattern = r'Writing to glpa \[\d+\] failed\.'  # 匹配 "Writing to glpa [数字] failed."
    matches = re.findall(pattern, s)  # 查找所有匹配的项
    return len(matches)  # 返回匹配项的数量

# 测试
s = "Writing to glpa [123] failed. Writing to glpa [456] failed. Writing to glpa [789] failed."
count = count_pattern_occurrences(s)
print(count)  # 输出: 3
